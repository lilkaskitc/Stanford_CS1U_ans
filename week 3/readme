1. <

  chmod +x read-input.py
  python2 ./read-input.py

  python2 ./read-input.py < nums-0-999

2. >, >>

  ls >temp
  ls >> temp

3. head, tail

  tail -n3  table
  head -n3  table
  head -n3  table | tail -2

4. tr

  tr -s "\n" < bad-new-line > good-new-line

5. sort

  tail -n +3 pumpkinsizes | sort -n -t "|" -k2 -r | head -3
  tail -n +3 pumpkinsizes | sort -n -t "|" -k2 -r | tail -2

6. uniq

  cat words-sorted | uniq >output1
  cat words-sorted | uniq -d -c > output2
  (tr ' ' '\n' < words) | sort |uniq >output3

